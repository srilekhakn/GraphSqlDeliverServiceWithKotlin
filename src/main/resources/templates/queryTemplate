query WarehouseDeliveryQueries {
  application
   warehouseDelivery(deliveryId: "101") {
    deliveryId
    product
    supplier
    quantity
    expectedDate
    expectedWarehouse
    deliveryStatus
  }
  allWarehouseDeliveries{
    deliveryId
    product
    supplier
    quantity
    expectedDate
    expectedWarehouse
    deliveryStatus
  }
  allWarehouseDeliveriesBydDeliveryStatus(deliveryStatus: true) {
    deliveryId
    product
    supplier
    quantity
    expectedDate
    expectedWarehouse
    deliveryStatus
  }
}